    "react-is": "^19.2.3",
    "react-leaflet": "^5.0.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^3.0.6",
    "recharts": "^3.6.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-mdx-frontmatter": "^5.2.0",
