{
  "name": "transparenta.eu",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "yarn --cwd docs-site install --frozen-lockfile",
    "dev": "NODE_OPTIONS=--max-http-header-size=64384 vite",
    "dev:lint": "concurrently \"vite\" \"tsc -w\" \"eslint ./src\"",
    "build": "tsc -b && vite build && yarn i18n:compile && yarn docs:build && node scripts/postbuild-docs.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "router:generate": "npx @tanstack/router-plugin --force",
    "docs:start": "yarn --cwd docs-site start",
    "docs:build": "yarn --cwd docs-site install --frozen-lockfile && yarn --cwd docs-site build",
    "docs:serve": "yarn --cwd docs-site serve",
    "docs:clear": "yarn --cwd docs-site clear",
    "docs:swizzle": "yarn --cwd docs-site swizzle",
    "release:notes": "node scripts/generate-release-post.cjs",
    "tutorials:test": "playwright test tests/tutorial",
    "tutorials:ui": "playwright test tests/tutorial --ui",
    "i18n:extract": "lingui extract",
    "i18n:compile": "lingui compile",
    "i18n:clean": "lingui extract --clean",
    "prepare": "husky"
  },
  "scripts-info": {
    "i18n:extract": "Scan source files for Lingui message IDs and update locale catalogs (adds new keys, keeps obsolete ones).",
    "i18n:compile": "Compile translated catalogs into runtime assets (JS/JSON) that the app loads in production.",
    "i18n:clean": "Extract and remove obsolete/unused message IDs from catalogs to keep them tidy."
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@docusaurus/core": "^3.8.1",
    "@docusaurus/preset-classic": "^3.8.1",
    "@docusaurus/theme-classic": "^3.8.1",
