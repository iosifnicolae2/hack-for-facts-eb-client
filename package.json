{
  "name": "transparenta.eu",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "HOME=$PWD yarn --cwd docs-site install --frozen-lockfile",
    "dev": "NODE_OPTIONS=--max-http-header-size=64384 vite",
    "dev:lint": "concurrently \"vite\" \"tsc -w\" \"eslint ./src\"",
    "build": "tsc -b && vite build && yarn i18n:compile && yarn docs:build && node scripts/postbuild-docs.js",
    "stt": "node dist/server/server.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc -b --noEmit",
    "learning:generate": "tsx scripts/generate-learning-paths.ts",
    "learning:validate": "tsx scripts/validate-learning-content.ts",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:integration": "playwright test --project=integration --workers=5",
    "test:integration:ui": "playwright test --project=integration --ui",
    "test:integration:debug": "playwright test --project=integration --debug",
    "test:e2e": "E2E_MODE=live playwright test --project=e2e",
    "test:e2e:ui": "E2E_MODE=live playwright test --project=e2e --ui",
    "test:e2e:snapshot": "E2E_MODE=replay playwright test --project=e2e",
    "test:e2e:snapshot:update": "E2E_MODE=record playwright test --project=e2e",
    "test:all": "playwright test",
    "router:generate": "node scripts/router-generate.mjs",
    "docs:start": "HOME=$PWD yarn --cwd docs-site start",
    "docs:build": "HOME=$PWD yarn --cwd docs-site install --frozen-lockfile && HOME=$PWD yarn --cwd docs-site build",
    "docs:serve": "HOME=$PWD yarn --cwd docs-site serve",
    "docs:clear": "HOME=$PWD yarn --cwd docs-site clear",
    "docs:swizzle": "HOME=$PWD yarn --cwd docs-site swizzle",
    "i18n:extract": "lingui extract",
    "i18n:compile": "lingui compile",
    "i18n:clean": "lingui extract --clean"
  },
  "scripts-info": {
    "i18n:extract": "Scan source files for Lingui message IDs and update locale catalogs (adds new keys, keeps obsolete ones).",
    "i18n:compile": "Compile translated catalogs into runtime assets (JS/JSON) that the app loads in production.",
    "i18n:clean": "Extract and remove obsolete/unused message IDs from catalogs to keep them tidy."
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.59.0",
