    "@tanstack/react-virtual": "^3.13.12",
    "@turf/bbox": "^7.2.0",
    "@turf/center": "^7.2.0",
    "@visx/annotation": "^3.12.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "d3-sankey": "^0.12.3",
    "date-fns": "^4.1.0",
    "file-saver": "^2.0.5",
    "fuse.js": "^7.1.0",
    "html-to-image": "^1.11.13",
    "immer": "^10.1.1",
