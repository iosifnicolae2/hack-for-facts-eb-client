    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@lingui/cli": "^5.7.0",
    "@lingui/swc-plugin": "5.10.0",
    "@lingui/vite-plugin": "^5.7.0",
    "@mdx-js/react": "^3.1.1",
    "@mdx-js/rollup": "^3.1.1",
